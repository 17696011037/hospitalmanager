<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyhc.hospitalmanager.dao.MedicalEventsMapper">
    <!--将检验结果添加到检验明细表中-->
    <insert id="addMedicalEvent">
         INSERT INTO `medical_events` VALUES(NULL,#{eventsName},#{unit},#{medicalResultData},#{medicalResult},#{checkResultId},#{upper_limit},#{lower_limit})
    </insert>
</mapper>